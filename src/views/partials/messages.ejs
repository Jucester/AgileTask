<% if (singupMessage.length > 0) { %>
    <div class="alert alert-warning">
        <%= singupMessage %> 
    </div>
<% } %>

<% if (singinMessage.length > 0) { %>
    <div class="alert alert-danger">
        <%= singinMessage %> 
    </div>
<% } %>

<% if (success.length > 0) { %>
    <div class="alert alert-success">
        <%= success %> 
    </div>
<% } %>

<% if (error.length > 0) { %>
    <div class="alert alert-danger">
        <%= error %> 
    </div>
<% } %>
